<Application x:Class="ResultStudioWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ResultStudioWPF"
             xmlns:viewModels="clr-namespace:ResultStudioWPF.ViewModels"
             Startup="Application_Startup">
    <Application.Resources>
        <!--  For mvvm light viewModel access  -->
        <viewModels:ViewModelLocator x:Key="Locator" />

        <BitmapImage x:Key="ErrorIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="/ResultStudioWPF;component/Images/red_round_error_warning_icon.png" />
        
    </Application.Resources>
</Application>
